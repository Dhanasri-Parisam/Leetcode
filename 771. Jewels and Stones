class Solution {
    public int numJewelsInStones(String jewels, String stones) {
        int cnt=0;
        for(int i=0;i<jewels.length();i++){
            for(int k=0;k<stones.length();k++){
                char r=jewels.charAt(i);
                char w=stones.charAt(k);
                if(r==w){
                    cnt++;
                }
            }
        }
        return cnt;
    }
}
