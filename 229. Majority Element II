class Solution {
public:
    vector<int> majorityElement(vector<int>& v) {
    int n=v.size();
    vector<int> ans;
    for(int i=0;i<n;i++){
        if (ans.size() == 0 || ans[0] != v[i]) {
                int cnt = 0;
                for (int j = 0; j < n; j++) {
                    if (v[j] == v[i]) {
                        cnt++;
                    }
                }
                if (cnt > (n / 3))
                    ans.push_back(v[i]);
            }

            if (ans.size() == 2) break;
        }
        return ans;
    }
};
